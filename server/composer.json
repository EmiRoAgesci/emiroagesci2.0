{
    "name": "gmazzap/wpstarter-simple-example",
    "description": "Tester emiroagesci 2.0 theme",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "wecodemore/wpstarter": "~2.0",
        "johnpbloch/wordpress": "4.6.1",
        "wpackagist-plugin/rest-api": "2.0-beta15",
        "wpackagist-plugin/w3-total-cache": "0.9.5.1",
        "wpackagist-plugin/category-posts": "4.6.2",
        "wpackagist-plugin/category-specific-rss-feed-menu": "v2.0",
        "wpackagist-plugin/xml-sitemap-feed": "4.7.4"
    },
    "require-dev": {
      "wp-cli/wp-cli" : "~0.22",
	    "psy/psysh" : "~0.6",
      "wpackagist-plugin/query-monitor": "2.13.*",
      "wpackagist-plugin/debug-bar": "*"
    },
    "config": {
        "vendor-dir": "wp-content/vendor",
        "optimize-autoloader": true
    },
    "scripts": {
        "post-install-cmd": "WCM\\WPStarter\\Setup::run",
        "post-update-cmd": "WCM\\WPStarter\\Setup::run"
    },
    "extra": {
        "wordpress-install-dir": "wp"
    }
}
